                        0,
                        -4
                    ],
                    "anchor_from": "bottom_middle",
                    "anchor_to": "bottom_middle",
                    "layer": 2
                }
            },
            {
                "percent@asa_zoom_percent": {}
            },
            {
                "asa_zoom_stack": {
                    "type": "stack_panel",
                    "size": [ "100%c", "100%cm" ],
                    "orientation": "horizontal",
                    "anchor_from": "bottom_right",
                    "anchor_to": "bottom_right",
                    "offset": [ -3, -3 ],
                    "controls": [
                        {
                            "verticle_stack_panel": {
                                "type": "panel",
                                "size": [ "100%cm", 22 ],
                                "controls": [
                                    {
                                        "slider_stack": {
                                            "type": "stack_panel",
                                            "size": [ "100%cm", "100%c" ],
                                            "anchor_from": "bottom_middle",
                                            "anchor_to": "bottom_middle",
                                            "controls": [
                                                {
                                                    "x_slider": {
                                                        "type": "panel",
                                                        "size": [ 103, 22 ],
                                                        "controls": [
                                                            {
                                                                "asa_x_slider@asa_slider": {
                                                                    "size": [ 90, 8 ],
                                                                    "layer": 50,
                                                                    "$asa_slider_steps": 21,
                                                                    "bindings": [
                                                                        {
                                                                            "binding_name":"#slider_steps"
                                                                        },
                                                                        {
                                                                            "binding_type": "view",
                                                                            "source_property_name": "((#slider_steps - #slider_steps) + 10)",
                                                                            "target_property_name": "#slider_value"
                                                                        }
                                                                    ]
                                                                }
                                                            },
                                                            {
                                                                "asa_move_text": {
                                                                    "type": "label",
                                                                    "anchor_from": "right_middle",
                                                                    "anchor_to": "left_middle",
                                                                    "shadow": true,
                                                                    "text": "Horizontal",
                                                                    "offset": [ 0, 0 ]
                                                                }
                                                            }
                                                        ]
                                                    }
                                                },
                                                {
                                                    "y_slider": {
                                                        "type": "panel",
                                                        "size": [ 103, 22 ],
                                                        "controls": [
                                                            {
                                                                "asa_y_slider@asa_slider": {
                                                                    "size": [ 90, 8 ],
                                                                    "layer": 50,
                                                                    "$asa_slider_steps": 21,
                                                                    "bindings": [
                                                                        {
                                                                            "binding_name":"#slider_steps"
                                                                        },
                                                                        {
                                                                            "binding_type": "view",
                                                                            "source_property_name": "((#slider_steps - #slider_steps) + 10)",
                                                                            "target_property_name": "#slider_value"
                                                                        }
                                                                    ]
                                                                }
                                                            },
                                                            {
                                                                "asa_move_text": {
                                                                    "type": "label",
                                                                    "anchor_from": "right_middle",
                                                                    "anchor_to": "left_middle",
                                                                    "shadow": true,
                                                                    "text": "Vertical",
                                                                    "offset": [ 0, 0 ]
                                                                }
                                                            }
                                                        ]
                                                    }
                                                },
                                                {
                                                    "zoom_slider_panel": {
                                                        "type": "panel",
                                                        "size": [ 138, 22 ],
                                                        "controls": [
                                                            {
                                                                "asa_zoom_slider@asa_slider": {
                                                                    "size": [ 125, 8 ],
                                                                    "layer": 50,
                                                                    "$asa_slider_steps": 21
                                                                }
                                                            }
                                                        ]
                                                    }
                                                }
                                            ]
                                        }
                                    }
                                ]
                            }
                        },
                        {
                            "asa_structure_zoom_out@asa_toggle_common.asa_toggle_nav_with_icon_template": {
                                "bindings": [],
                                "$toggle_name": "asa_structure_zoom",
                                "$asa_ignored_outline": true,
                                "$toggle_group_forced_index": 0,
                                "reset_on_focus_lost": false,
                                "layer": 50,
                                "$asa_icon_texture": "textures/ui/close_button_default",
                                "size": [ 22, 22 ]
                            }
                        }
                    ]
                }
            }
        ]
    },

