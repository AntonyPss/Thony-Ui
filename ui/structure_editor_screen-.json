                }
            }
        ]
    },

    "asa_structure_renderer": {
        "type": "panel",
        "size": [ "100%", "100% - 31px" ],
        "anchor_from": "bottom_left",
        "anchor_to": "bottom_left",
        "controls": [
            {
                "asa_custom_structure_renderer@structure_editor.asa_custom_structure_renderer": {
