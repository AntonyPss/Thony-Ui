{
  "namespace": "general_section",

  //---------------------------------------------------------------------------
  // Common Section
  //---------------------------------------------------------------------------

  "section_image_header": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", 32 ],

    "$section_image_line|default": false,
    "$section_image_line_texture|default": "textures/ui/creator_glyph_color",
    "$section_image_icon|default": "textures/ui/icon_settings",
    "$section_image_size|default": [ 28, 28 ],
    "$section_image_padd|default": [ 4, 0 ],
    "$section_title|default": "#none",
    "controls": [
      {
        "section_image_panel": {
          "type": "panel",
          "size": [ 32, "100%" ],
          "controls": [
            {
              "section_image_icon": {
                "type": "image",
                "texture": "$section_image_icon",
                "anchor_from": "center",
                "anchor_to": "center",
                "layer": 4,
                "size": "$section_image_size"
              }
            },
            {
              "section_image_line": {
                "type": "image",
                "texture": "$section_image_line_texture",
                "ignored": "(not $section_image_line)",
                "anchor_from": "center",
                "anchor_to": "center",
                "layer": 8,
                "size": "$section_image_size"
              }
            }
          ]
        }
      },
      {
        "section_image_padding": {
          "type": "panel",
          "size": "$section_image_padd"
        }
      },
      {
        "section_title_panel": {
          "type": "panel",
          "size": [ "100%c", "100%" ],
          "controls": [
            {
              "section_title": {
                "type": "label",
                "text": "$section_title",
                "font_type": "MinecraftTen",
                "font_scale_factor": 1.5,
                "anchor_from": "center",
                "anchor_to": "center",
                "layer": 4,
                "shadow": true,
                "color": "$main_header_text_color",
                "size": [ "default", 18 ],
                "offset": [ 0, -2 ]
              }
            }
          ]
        }
      }
    ]
  },

  "padding_4": {
    "type": "panel",
    "size": [ 4, 0 ]
  },

  //---------------------------------------------------------------------------
  // Account Section
  //---------------------------------------------------------------------------

  "gamerpic_status": {
    "type": "panel",
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "size": [ 13, 13 ],
    "offset": [ 3, 0 ],
    "controls": [
      {
        "status_online": {
          "type": "image",
          "texture": "textures/ui/online",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "layer": 4,
          "size": [ 9, 9 ],
          "bindings": [
            {
              "binding_name": "#logged_in",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "status_offline_bg": {
          "type": "image",
          "texture": "textures/ui/tooltip_default_background",
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "layer": 4,
          "size": [ 11, 11 ],
          "bindings": [
            {
              "binding_name": "#not_logged_in",
              "binding_name_override": "#visible"
            }
          ],
          "controls": [
            {
              "status_offline": {
                "type": "image",
                "texture": "textures/ui/offline",
                "anchor_from": "center",
                "anchor_to": "center",
                "layer": 2,
                "size": [ 9, 9 ]
              }
            }
          ]
        }
      }
    ]
  },

  "gamerpic_image": {
    "type": "image",
    "texture": "#texture",
    "size": [ 48, 48 ],
    "bindings": [
      {
        "binding_name": "#gamerpic_texture_path",
        "binding_name_override": "#texture"
      },
      {
        "binding_name": "#gamerpic_texture_location_type",
        "binding_name_override": "#texture_file_system"
      }
    ]
  },

  "gamerpic_texture_button@common.button": {
    "$pressed_button_name": "button.gamerpic",
    "size": [ 48, 48 ],
    "controls": [
      {
        "default@common.focus_border_white": {
          "color": "black",
          "controls": [
            {
              "gamerpic_status@general_section.gamerpic_status": {
                "layer": 4
              }
            },
            {
              "gamerpic@general_section.gamerpic_image": {
                "layer": -1
              }
            }
          ]
        }
      },
      {
        "hover@common.focus_border_white": {
          "controls": [
            {
              "gamerpic_status@general_section.gamerpic_status": {
                "layer": 4
              }
            },
            {
              "gamerpic@general_section.gamerpic_image": {
                "layer": -1
              }
            }
          ]
        }
      },
      {
        "pressed@common.focus_border_white": {
          "controls": [
            {
              "gamerpic_status@general_section.gamerpic_status": {
                "layer": 4
              }
            },
            {
              "gamerpic@general_section.gamerpic_image": {
                "alpha": 0.8
              }
            }
          ]
        }
      }
    ]
  },

  "gamertag_label_panel": {
    "type": "stack_panel",
    "size": [ "fill", "100%cm" ],
    "controls": [
      {
        "gamertag_title": {
          "type": "label",
          "text": "options.gamertag",
          "font_type": "MinecraftTen",
          "font_scale_factor": 1.28,
          "layer": 2,
          "shadow": true,
          "color": "$main_header_text_color",
          "size": [ "100%", 16 ]
        }
      },
      {
        "gamertag_text_logged": {
          "type": "label",
          "text": "#gamertag_label",
          "font_type": "smooth",
          "layer": 2,
          "color": "$body_text_color",
          "size": [ "100%", 10 ],
          "bindings": [
            {
              "binding_name": "#gamertag_label"
            },
            {
              "binding_name": "#logged_in",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "gamertag_text_notlogged": {
          "type": "label",
          "text": "xbox.connection.error",
          "font_type": "smooth",
          "layer": 2,
          "shadow": true,
          "color": "$main_header_text_color",
          "size": [ "100%", 10 ],
          "bindings": [
            {
              "binding_name": "#not_logged_in",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "mc_account_id_text": {
          "type": "label",
          "text": "#MinecraftAccount_id",
          "font_type": "smooth",
          "font_scale_factor": 0.9,
          "layer": 2,
          "alpha": 0.9,
          "color": "$body_text_color",
          "size": [ "100%", "default" ],
          "bindings": [
            {
              "binding_name": "#MinecraftAccount_id"
            }
          ]
        }
      },
      {
        "device_id_text": {
          "type": "label",
          "text": "#device_id",
          "font_type": "smooth",
          "font_scale_factor": 0.9,
          "layer": 2,
          "alpha": 0.9,
          "color": "$body_text_color",
          "size": [ "100%", "default" ],
          "bindings": [
            {
              "binding_name": "#device_id"
            }
          ]
        }
      }
    ]
  },

  "gamertag_controls": {
    "orientation": "horizontal",
    "layer": 2,
    "size": [ "100%", "100%cm" ],
    "controls": [
      { "button@general_section.gamerpic_texture_button": {} },
      { "pad0@padding_4": {} },
      { "panel@general_section.gamertag_label_panel": {} }
    ]
  },


  "sign_out_gamertag_controls": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "layer": 2,
    "size": [ "100%", "100%cm" ],
    "controls": [
      { "button@general_section.gamerpic_texture_button": {} },
      { "pad0@padding_4": {} },
      { "panel@general_section.gamertag_label_panel": {} }
    ]
  },

