                    }
                  },
                  {
                    "manage_account_button_mobile@settings_common.link_button": {
                      "$default_button_texture": "textures/ui/button_borderless_dark",
                      "$hover_button_texture": "textures/ui/button_borderless_darkhover",
                      "$pressed_button_texture": "textures/ui/button_borderless_darkpressed",
                      "$default_text_color": "$dark_button_default_text_color",
                      "ignored": "(not ($is_ios or $is_android))",
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "$button_text": "options.manageAccount",
                      "size": [ "100%", 28 ],
                      "property_bag": {
                        "#hyperlink": "https://account.xbox.com/Settings"
                      }
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "manage_invites_panel": {
          "type": "panel",
          "size": [ "100%", 28 ],
          "controls": [
            {
              "manage_invites_popup_button@common_buttons.dark_text_button": {
                "ignored": "(not $is_holographic and not $fire_tv and not $thirdpartyconsole)",
