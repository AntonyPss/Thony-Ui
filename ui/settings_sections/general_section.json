        "sign_out_button@common_buttons.dark_text_button": {
          "anchor_from": "center",
          "anchor_to": "center",
          "$pressed_button_name": "sign_in_button",
          "$button_text": "accounts.signOut",
          "size": [ "100%", 28 ]
        }
      },
      {
        "delete_account@common_buttons.dark_text_button": {
          "ignored": "((not $is_pregame) or (not $is_ios))",
