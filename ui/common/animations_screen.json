{
  "namespace": "animations",

  "anim_alpha": {
    "anim_type": "alpha",
    "easing": "$animAlphaEasing",
    "duration": "$animAlphaDuration",
    "from": "$animAlphaFrom",
    "to": "$animAlphaTo"
  },

  "anim_size": {
    "anim_type": "size",
    "easing": "$animSizeEasing",
    "duration": "$animSizeDuration",
    "from": "$animSizeFrom",
    "to": "$animSizeTo"
  },

  "anim_offset": {
    "anim_type": "offset",
    "easing": "$animOffsetEasing",
    "duration": "$animOffsetDuration",
    "from": "$animOffsetFrom",
    "to": "$animOffsetTo"
  },

  "anim_header_offset": {
    "anim_type": "offset",
    "easing": "in_out_expo",
    "duration": 0.5,
    "from": [0, -200],
    "to": [0, 0]
  },

  "anim_header_alpha": {
    "anim_type": "offset",
    "easing": "in_out_sine",
    "duration": 0.4,
    "from": 0.0,
    "to": 1.0
  },

  "anim_footer_offset": {
    "anim_type": "offset",
    "easing": "in_expo",
    "duration": 0.4,
    "from": [0, "16%"],
    "to": [0, 0]
  }
}
