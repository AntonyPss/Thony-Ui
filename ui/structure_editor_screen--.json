{
  "namespace": "structure_editor",

  "help_icon": {
    "size": [ 14, 16 ]
  },

  "fullscreen_header": {
    "type": "stack_panel",
    "size": [ "100%", "100%" ],
    "controls": [
      {
        "top_bar": {
          "type": "image",
          "texture": "textures/ui/StoreTopBar",
          "layer": 1,
          "size": [ "100%", 24 ],
          "controls": [
            {
              "title_stack_panel": {
                "type": "stack_panel",
                "orientation": "horizontal",
                "size": [ "100%", 20 ],
                "controls": [
                  {
                    "p1": {
                      "type": "panel",
                      "size": [ 2, 0 ]
                    }
                  },
                  {
                    "back_button@common.back_content_panel": {}
                  },
                  {
                    "p2": {
                      "type": "panel",
                      "size": [ 2, 0 ]
                    }
                  },
                  {
                    "title_panel": {
                      "type": "panel",
                      "size": [ "100%c", "100%" ],
                      "controls": [
                        {
                          "text": {
                            "type": "label",
                            "text": "structure_block.title",
                            "font_scale_factor": 1.4,
                            "font_type": "MinecraftTen",
                            "layer": 4,
                            "color": "$title_text_color",
                            "size": [ "default", "100%" ],
                            "offset": [ 0, -1 ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "fill_padding": {
                      "type": "panel",
                      "size": [ "fill", 0 ]
                    }
                  },
                  {
                    "buttons_panel": {
                      "type": "panel",
                      "size": [ "100%cm", "100%c" ],
                      "controls": [
                        {
                          "stack_content": {
                            "type": "stack_panel",
                            "orientation": "horizontal",
                            "size": [ "100%c", 18 ],
                            "offset": [ -2, -1 ],
                            "controls": [
                              {
                                "structure_zoom_toggle@common_toggles.light_text_toggle": {
                                  "$button_text": "Z",
                                  "size": [ 18, 18 ]
                                }
                              },
                              {
                                "help_button@common_buttons.light_content_button": {
                                  "$pressed_button_name": "button.help",
                                  "$button_content": "structure_editor.help_icon",
                                  "size": [ 18, 18 ]
                                }
                              }
                            ]
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "main": {
          "type": "panel",
          "size": [ "100%", "fill" ],
          "controls": [
            {
              "divider_content@divider_content": {}
            }
          ]
        }
      }
    ]
  },

  "background": {
    "type": "image",
    "texture": "textures/ui/Black",
    "alpha": 0.5
  },

  "scrolling_panel_wrapper@structure_editor.background": {
    "offset": [ 0, 0 ]
  },

  "scrolling_panel_wrapper/scrolling_panel": {
    "size": [ "100% - 4px", "100% - 4px" ]
  },

  "image_panel_wrapper@structure_editor.background": {},

  "image_panel_wrapper/image_panel": {
    "size": [ "100%", "100% - 8px" ]
  },

  "divider_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "100%" ]
  },

  "divider_content/left_side": {
    "size": [ 132, "100% - 4px" ]
  },

  "divider_content/right_side": {
    "size": [ "fill", "100% - 4px" ]
  },

  "structure_editor_content": {
    "controls": [
      { "fullscreen_header@fullscreen_header": {} }
    ]
  },

  "structure_editor_screen": {
    "$header_bar_control": "common_assets.store_top_bar",
    "$header_safezone_control": "common_assets.store_top_bar_filler",
    "$screen_background_alpha": 0.7
  }
}