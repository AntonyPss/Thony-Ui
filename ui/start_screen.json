{
  "namespace": "start",

  //---------------------------------------------------------------------------
  // Images Section
  //---------------------------------------------------------------------------

  "realms_icon": {
    "type": "image",
    "texture": "textures/ui/realmsIcon",
    "layer": 2,
    "size": [ 11, 14 ]
  },

  "servers_icon": {
    "type": "image",
    "texture": "textures/ui/servers",
    "layer": 2,
    "size": [ 16, 16 ]
  },

  "Xbox_icon": {
    "size": [ 15, 15 ],
    "offset": [ 0.5, 0.5 ]
  },

  "change_skin_icon": {
    "texture": "textures/ui/icon_hangar",
    "size": [ 16, 16 ]
  },

  "profile_icon": {
    "type": "image",
    "texture": "textures/ui/icon_steve",
    "layer": 2,
    "size": [ 12, 12 ]
  },

  //---------------------------------------------------------------------------
  // Text Panel
  //---------------------------------------------------------------------------

  "version_label@common_assets.label_background": {
    "$label_anchor": "top_right",
    "$label_text": "#version",

    "$anim_easing_offset": "in_quart",
    "$anim_offset_duration": 0.8,
    "$anim_offset_from": [ "50%", 0 ],
    "$anim_offset_to": [ 0, 0 ],

    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset"
    ],
    "$label_bindings": [
      {
        "binding_name": "#version"
      }
    ]
  },

  "copyright_label@common_assets.label_background": {
    "$label_anchor": "top_left",
    "$label_text": "menu.copyright",
    "$label_text_offset": [ -0.5, 0 ],

    "$anim_easing_offset": "in_quart",
    "$anim_offset_duration": 0.8,
    "$anim_offset_from": [ "-50%", 0 ],
    "$anim_offset_to": [ 0, 0 ],

    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset"
    ]
  },

  "creator_label@common_assets.label_background": {
    "$label_anchor": "bottom_left",
    "$label_text": "menu.creator",

    "$anim_easing_offset": "in_quart",
    "$anim_offset_duration": 1.5,
    "$anim_offset_from": [ "-50%", 0 ],
    "$anim_offset_to": [ 0, 0 ],

    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset"
    ]
  },

  "d_version_label@common_assets.label_background": {
    "$label_anchor": "bottom_right",
    "$label_text": "#development_version",
    "ignored": "$is_publish",

    "$anim_easing_offset": "in_quart",
    "$anim_offset_duration": 1.5,
    "$anim_offset_from": [ "50%", 0 ],
    "$anim_offset_to": [ 0, 0 ],

    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset"
    ],
    "$label_bindings": [
      {
        "button_name": "#development_version"
      }
    ]
  },

  "pack_version_label@common_assets.label_background": {
    "$label_anchor": "bottom_right",
    "$label_text": "$menu_creator_version",
    "ignored": "(not $is_publish)",

    "$anim_easing_offset": "in_quart",
    "$anim_offset_duration": 1.5,
    "$anim_offset_from": [ "50%", 0 ],
    "$anim_offset_to": [ 0, 0 ],

    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset"
    ]
  },

  "emoji_text@common_assets.label_background": {
    "$label_background": "textures/ui/hud_tip_text_background",
    "$label_anchor": "bottom_middle",
    "$label_text": "->.<-",

    "disable_anim_fast_forward": false,
    "anims": [
      "@start.emoji_anim_show"
    ]
  },

  "emoji_anim_show": {
    "anim_type": "offset",
    "easing": "in_quart",
    "duration": 2.0,
    "from": [ 0, 128 ],
    "to": [ 0, 0 ],
    "next": "@start.emoji_anim_wait"
  },

  "emoji_anim_wait": {
    "anim_type": "wait",
    "duration": 4.0,
    "next": "@start.emoji_anim_hide"
  },

  "emoji_anim_hide": {
    "anim_type": "offset",
    "easing": "out_quart",
    "duration": 2.0,
    "from": [ 0, 0 ],
    "to": [ 0, 128 ]
  },

  "text_panel": {
    "size": [ "100% - 2px", 28 ],
    "controls": [
      { "version@start.version_label": {} },
      { "copyright@start.copyright_label": {} },
      { "creator@start.creator_label": {} },
      { "d_version@start.d_version_label": {} },
      { "pack_version@start.pack_version_label": {} },
      { "emoji_text@start.emoji_text": {} }
    ]
  },

  //---------------------------------------------------------------------------
  // Skin Viewer Panel
  //---------------------------------------------------------------------------

  "skin_viewer_panel": {
    "modifications": [
      {
        "control_name": "paper_doll_name_tag",
        "operation": "replace",
        "value": [
          {
            "paper_doll_name_tag_bg": {
              "type": "image",
              "texture": "textures/ui/hud_tip_text_background",
              "anchor_from": "top_middle",
              "anchor_to": "bottom_middle",
              "layer": 2,
              "alpha": 0.5,
              "size": [ "100%c + 8px", 14 ],
              "max_size": [ 132, 14 ],
              "controls": [
                {
                  "paper_doll_name_tag_label": {
                    "type": "label",
                    "text": "#playername",
                    "anchor_from": "center",
                    "anchor_to": "center",
                    "layer": 4,
                    "shadow": true,
                    "color": "$main_header_text_color",
                    "size": [ "default", "default" ],
                    "bindings": [
                      {
                        "binding_name": "#playername"
                      }
                    ]
                  }
                }
              ]
            } 
          }
        ]
      }
    ]
  },

  "change_skin_button@common_buttons.dark_text_button": {
    "$button_type_panel": "common_assets.icon_wrapper",
    "$pressed_button_name": "button.menu_skins",
    "$button_text": "menu.skins",
    "$button_content": "start.change_skin_icon",
    "size": [ "100%", "100%" ]
  },

  "dressing_room_dark_button@common_buttons.light_content_button": {
    "$button_type_panel": "common_assets.icon_wrapper",
    "$pressed_button_name": "button.menu_skins",
    "$button_text": "menu.skins",
    "$button_content": "start.change_skin_icon",
    "size": [ 90, 26 ]
  },

  "skin_or_profile_panel": {
    "modifications": [
      {
        "control_name": "dressing_room_button",
        "operation": "replace",
        "value": [
          {
            "dressing_room@start.dressing_room_dark_button": {}
          }
        ]
      }
    ]
  },

  "skin_panel": {
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "size": [ "32% - 12px", 116 ],

    "$anim_easing_offset": "in_out_cubic",
    "$anim_offset_duration": 0.8,
    "$anim_offset_from": [ 200, 0 ],
    "$anim_offset_to": [ 0, 24 ],

    "$anim_easing_alpha": "in_out_circ",
    "$anim_alpha_duration": 0.6,
    "$anim_alpha_from": 0.0,
    "$anim_alpha_to": 1.0,

    "animation_reset_name": "screen_animation_reset",
    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset",
      "@animations.anim_alpha"
    ]
  },

  //---------------------------------------------------------------------------
  // lowers Buttons Panel
  //---------------------------------------------------------------------------

  "profile_button": {
    "$button_content_size_override": [ "100%c + 4px", 22 ]
  },

  "inbox_button": {
    "size": [ "100%y", 26 ]
  },

  "inbox_icon_button": {
    "size": [ "100%y", 26 ]
  },

  "inbox_button_unread_panel": {
    "offset": [ 2, -2 ]
  },

  "gamertag_label": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "size": [ "100%c", 26 ],

    "$anim_easing_offset": "in_out_cubic",
    "$anim_offset_duration": 0.7,
    "$anim_offset_from": [ 0, 100 ],
    "$anim_offset_to": [ 8, -38 ],

    "$anim_easing_alpha": "in_out_sine",
    "$anim_alpha_duration": 0.7,
    "$anim_alpha_from": 0.0,
    "$anim_alpha_to": 1.0,

    "animation_reset_name": "screen_animation_reset",
    "disable_anim_fast_forward": false,
    "anims": [
      "@animations.anim_offset",
      "@animations.anim_alpha"
    ],
    "controls": [
      {
        "inbox_icon_button@start.inbox_icon_button": {
          "bindings": [
            {
              "binding_name": "#inbox_enabled",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "padding": {
          "type": "panel",
          "size": [ 4, 0 ],
          "bindings": [
            {
              "binding_name": "#inbox_enabled",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "profile_button@start.profile_button": {
          "size": [ "100%c", "100%" ],
          "bindings": [
            {
              "binding_name": "#profile_button_a_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

  //---------------------------------------------------------------------------
  // Main Buttons Panel
  //---------------------------------------------------------------------------

  "stacked_row": {
    "size": [ "100%", 29 ]
  },

  "main_buttons_stack_panel": {
    "modifications": [
      {
        "control_name": "title_panel_content",
        "operation": "replace",
        "value": [
          {
            "title_panel": {
              "type": "panel",
              "size": [ "100%", "100%c" ],
              "controls": [
                {
                  "title_panel_content@common_art.start_title_panel_content": {
                    "$anim_easing_alpha": "in_out_sine",
                    "$anim_alpha_duration": 0.6,
                    "$anim_alpha_from": 0.0,
                    "$anim_alpha_to": 1.0,

                    "$anim_easing_offset": "out_back",
                    "$anim_offset_duration": 0.8,
                    "$anim_offset_from": [ 0, -100 ],
                    "$anim_offset_to": [ 0, 0 ],

                    "animation_reset_name": "screen_animation_reset",
                    "disable_anim_fast_forward": false,
                    "propagate_alpha": true,
                    "layer": 99,
                    "anims": [
                      "@animations.anim_alpha",
                      "@animations.anim_offset"
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  },

  "main_buttons_panel": {
    "$anim_easing_alpha": "out_cubic",
    "$anim_alpha_duration": 0.3,
    "$anim_alpha_from": 0.0,
    "$anim_alpha_to": 1.0,

    "$anim_easing_offset": "in_out_expo",
    "$anim_offset_duration": 0.5,
    "$anim_offset_from": [ 0, -200 ],
    "$anim_offset_to": [ 0, 0 ],

    "animation_reset_name": "screen_animation_reset",
    "disable_anim_fast_forward": false,
    "propagate_alpha": true,
    "anims": [
      "@animations.anim_alpha",
      "@animations.anim_offset"
    ]
  },

  "main_buttons_panel/stacked_rows": {
    "modifications": [
      {
        "control_name": "featured_world",
        "operation": "remove"
      },
      {
        "control_name": "settings",
        "operation": "remove"
      },
      {
        "control_name": "stacked_row_2",
        "operation": "replace",
        "value": [
          {
            "settings_button_panel@start.stacked_row": {
              "controls": [
                {
                  "settings_button@start.settings_button": {}
                }
              ]
            }
          }
        ]
      },
      {
        "control_name": "settings_and_editions_panel",
        "operation": "replace",
        "value": [
          {
            "store_button_panel@start.stacked_row": {
              "controls": [
                {
                  "store_button@start.store_button": {
                    "enabled": "(not $store_disabled)"
                  }
                }
              ]
            }
          }
        ]
      },
      {
        "control_name": "store",
        "operation": "replace",
        "value": [
          {
            "exit_button_panel@start.stacked_row": {
              "controls": [
                {
                  "exit_button@start.main_button_dark": {
                    "$hover_button_texture": "textures/ui/button_red",
                    "$pressed_button_name": "button.menu_exit",
                    "$button_text": "gui.exit",
                    "size": [ "100% - 2px", "100% - 2px" ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  },

  "main_buttons_panel/stacked_rows/realms/realms_button": {
    "$button_type_panel": "common_assets.icon_wrapper",
    "$button_content": "start.realms_icon"
  },

  //---------------------------------------------------------------------------
  // Upper Buttons Panel
  //---------------------------------------------------------------------------

  "upper_button@common_assets.dark_text_hover_button": {
    "size": [ "100% - 4px", "100%x" ]
  },

  "featured_world_button@start.upper_button": {
    "$pressed_button_name": "button.featured_category",
    "$button_text": "start_screen.start_demo",
    "$button_content": "start.worlds_icon"
  },

  "server_button@start.upper_button": {
    "$pressed_button_name": "button.menu_servers",
    "$button_text": "menu.servers",
    "$button_content": "start.servers_icon"
  },

  "feedback_button@start.upper_button": {
    "$pressed_button_name": "button.feedback_button_pressed",
    "$button_text": "gui.submitFeedback.button.text",
    "$button_content": "start.feedback_icon"
  },

  "achievements_button@start.upper_button": {
    "$pressed_button_name": "button.menu_achievements",
    "$button_text": "menu.achievements",
    "$button_content": "start.achievements_icon"
  },

  "xbl_signin_button@start.upper_button": {
    "$pressed_button_name": "button.signin",
    "$button_text": "xbox.signin",
    "$button_content": "start.Xbox_icon",
    "bindings": [
      {
        "binding_name": "#sign_in_visible",
        "binding_name_override": "#visible"
      }
    ]
  },

  "upper_stack": {
    "type": "panel",
    "size": [ "100%y", 28 ]
  },

  "upper_buttons_panel": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "size": [ 26, "100%c" ],

    "$anim_easing_offset": "in_out_sine",
    "$anim_offset_duration": 1.0,
    "$anim_offset_from": [ 0, 100 ],
    "$anim_offset_to": [ 0, 0 ],
    "animation_reset_name": "screen_animation_reset",
    "disable_anim_fast_forward": false,
    "anims": [ "@animations.anim_offset" ],
    "controls": [
      {
        "featured_world_button@start.upper_stack": {
          "ignored": "$featured_world_ignored",
          "controls": [
            { "featured_world_button@start.featured_world_button": {} }
          ]
        }
      },
      {
        "servers_button@start.upper_stack": {
          "controls": [
            { "server_button@start.server_button": {} }
          ]
        }
      },
      {
        "feedback_button@start.upper_stack": {
          "ignored": "$feedback_ignored",
          "controls": [
            { "feedback_button@start.feedback_button": {} }
          ]
        }
      },
      {
        "achievements_button@start.upper_stack": {
          "ignored": "$achievements_ignored",
          "controls": [
            { "achievements_button@start.achievements_button": {} }
          ]
        }
      },
      {
        "xbl_signin_button@start.upper_stack": {
          "ignored": "$achievements_ignored",
          "controls": [
            { "xbl_signin_button@start.xbl_signin_button": {} }
          ]
        }
      }
    ]
  },

  //---------------------------------------------------------------------------
  // Screen Content
  //---------------------------------------------------------------------------

  "start_screen_content": {
    "$default_button_texture": "textures/ui/button_borderless_dark",
    "$hover_button_texture": "textures/ui/button_borderless_darkhover",
    "$pressed_button_texture": "textures/ui/button_borderless_darkpressed",

    "$default_text_color": "$dark_button_default_text_color",
    "$hover_text_color": "$dark_button_hover_text_color",
    "$pressed_text_color": "$dark_button_pressed_text_color",
    "modifications": [
      {
        "control_name": "online_button_stack",
        "operation": "remove"
      },
      {
        "array_name": "controls",
        "operation": "insert_front",
        "value": [
          {
            "input_info_panel@common_assets.input_info_panel": {
              "layer": 2
            }
          },
          {
            "upper_buttons@start.upper_buttons_panel": {
              "layer": 3
            }
          },
          {
            "gamertag@start.gamertag_label": {
              "layer": 4
            }
          }
        ]
      }
    ]
  }
}